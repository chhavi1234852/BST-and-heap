[11:59 pm, 15/11/2025] Chhavi: #include <iostream>
#include <climits>
using namespace std;
`class Node{
public:
    int data;
    Node* left;
    Node* right;
    Node(int v){ data=v; left=right=NULL; }
};
bool check(Node* r,int mn,int mx){
    if(!r) return true;
    if(r->data<=mn || r->data>=mx) return false;
    return check(r->left,mn,r->data) && check(r->right,r->data,mx);
}
bool isBST(Node* r){
    return check(r,INT_MIN,INT_MAX);
}

int main(){
    Node* root=new Node(10);
    root->left=new Node(5);
    root->right=new Node(20);
    cout<<(isBST(root)?"YES":"NO");
}
